{
  "hosting": {
    "public": "dist/gym-app",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "predeploy": [
      "ng lint",
      "ng build --prod"
    ],
    "rewrites": [
      {
        "source": "/api/superAdmin/**",
        "function": "superAdmin"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "predeploy": [
      "npm --prefix functions run lint",
      "npm --prefix functions run build"
    ]
  }
}
